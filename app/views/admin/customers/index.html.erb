<h1>顧客一覧</h1>
    <div>
    <% @customers.each do |customer_list| %>
        <%= link_to admin_customer_path(customer_list.id) do %>
            <p>氏名:<%= customer_list.last_name %> <%= customer_list.first_name %></p>
                    <%= customer_list.last_name_kana %> <%= customer_list.first_name_kana %><br>
        <% end %>
        <h4>住所</h4><%= customer_list.postal_code %><br><%= customer_list.address %><br>
        <h4>電話番号</h4><%= customer_list.telephone_number %>
    <% end %>
    </div>