<h2>ジャンル名一覧・追加</h2><br>
<table>
    <tr>
<%= form_with model: @genre, url: admin_genres_path, local:true do |f| %>
        <th>ジャンル名</th><td><%= f.text_field :name %></td><td><%= f.submit '新規登録' %></td>
<% end %>
    </tr>
</table>
<h3>ジャンル名</h3>
<% @genres.each do |genre| %>
    <p><%= genre.name %><%= link_to "編集", edit_admin_genre_path(genre.id) %></p> 
<% end %>